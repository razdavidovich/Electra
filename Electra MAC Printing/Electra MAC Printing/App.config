  </startup>
  <Settings>
    <DataBase>
      <add key="DefaultDatabase" value="ElectraMACPrintingDB" />
    </DataBase>
    <LogSettings>
      <add key="LogFilePath" value="C:\Support\Logs\RofinLaserMarking\" />
      <add key="LogFileName" value="RofinLaserMarking_Log" />
