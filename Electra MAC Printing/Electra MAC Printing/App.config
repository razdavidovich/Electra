  <connectionStrings>
    <add name="ElectraMACPrintingDB" connectionString="data source=.\SQLEXPRESS;Initial Catalog=ElectraMACPrinting;User ID=sa;Password=eureka" providerName="System.Data.SqlClient" />
  </connectionStrings>

  <startup>
