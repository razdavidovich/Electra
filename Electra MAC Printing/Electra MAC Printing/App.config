  <connectionStrings>
    <add name="ElectraMACPrintingDB" connectionString="data source=.\SQLEXPRESS;Initial Catalog=ElectraMACPrinting;User ID=sa;Password=eureka" providerName="System.Data.SqlClient" />
  </connectionStrings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <Settings>
    <DataBase>
      <add key="DefaultDatabase" value="ElectraMACPrintingDB" />
    </DataBase>
    <LogSettings>
      <add key="LogFilePath" value="C:\Support\Logs\RofinLaserMarking\" />
      <add key="LogFileName" value="RofinLaserMarking_Log" />
      <add key="LogFileExtension" value="txt" />
    </LogSettings>
    <GetSetGeneralSettings>
      <add key="StationName" value="PrintingStation 1" />
      <add key="UnitSettings" value="COM3,115200,Even,8,One" />
      <add key="ModbusSlaveAddress" value="2" />
      <add key="SerialNumberAddress" value="0x0000" />
      <add key="DataAddress" value="0x0010" />
      <add key="PrinterSettings" value="COM10,19200,None,8,One" />
      <add key="TimeOut" value="1000" />
    </GetSetGeneralSettings>

    
    
    
    <UserRoleSettings>
      <add key="adminRights" value="1,2" />
      <add key="userRights" value="3" />
    </UserRoleSettings>
    <OtherSettings>
      <!--The timer values must be in millisconds-->
      <add key="FrmAppWizardActiveTabKey" value="login" />
      <add key="ToolBarSettingsVisiable" value="false" />
    </OtherSettings>


  </Settings>

  <SerialPortSettings>
    <SettingBaudRate>
      <add key="1200" value="1200" />
      <add key="2400" value="2400" />
      <add key="4800" value="4800" />
      <add key="9600" value="9600" />
      <add key="19200" value="19200" />
      <add key="38400" value="38400" />
      <add key="57600" value="57600" />
      <add key="115200" value="115200" />
    </SettingBaudRate>
    <SettingDataBits>
      <add key="7" value="7" />
      <add key="8" value="8" />
      <add key="9" value="9" />
    </SettingDataBits>
  </SerialPortSettings>
  
  
  <LanguageSupport>
    <LanguageCodes>
      <add key="Default" value="en" />
    </LanguageCodes>
    <LanguageDirection>
      <!--LTR : 0 ,RTL : 1 -->
      <add key="Default" value="0" />
    </LanguageDirection>
  </LanguageSupport>

  <Messages>
    <ID_0>
      <add key="MessageTitle" value="Settings" />
